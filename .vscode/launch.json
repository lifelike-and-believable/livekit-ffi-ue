{
  // Debugging Unreal Editor with the LiveKitSandbox project
  "version": "0.2.0",
  "configurations": [
    {
      "name": "UE5 Editor - LiveKitSandbox (env UE5_EDITOR_EXE)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${env:UE5_EDITOR_EXE}",
      "args": [
        "${workspaceFolder}\\ue\\LiveKitSandbox\\LiveKitSandbox.uproject"
      ],
      "cwd": "${workspaceFolder}\\ue\\LiveKitSandbox",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "preLaunchTask": "Prepare LiveKit Sandbox"
    },
    {
      "name": "UE5 Editor - LiveKitSandbox (default path)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "C:\\Program Files\\Epic Games\\UE_5.6\\Engine\\Binaries\\Win64\\UnrealEditor.exe",
      "args": [
        "${workspaceFolder}\\ue\\LiveKitSandbox\\LiveKitSandbox.uproject"
      ],
      "cwd": "${workspaceFolder}\\ue\\LiveKitSandbox",
      "console": "integratedTerminal",
      "stopAtEntry": false,
      "preLaunchTask": "Prepare LiveKit Sandbox"
    },
    {
      "name": "Attach to UE5 Editor",
      "type": "cppvsdbg",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
